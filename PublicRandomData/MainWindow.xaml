<Window x:Class="PublicRandomData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PublicRandomData"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="500">
    <Grid >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Vertical" Background="#88000000">
            <Button Content="测试实例" Tag="Demo" Margin="10" Height="30" Width="80" Click="Button_Click"/>
            <Button Content="表格实例" Tag="Table" Margin="10" Height="30" Width="80" Click="Button_Click"/>
            <Button Content="添加水印" Tag="Watermark" Margin="10" Height="30" Width="80" Click="Button_Click"/>
            <Button Content="压缩文件" Tag="ZIP" Margin="10" Height="30" Width="80" Click="Button_Click"/>
            <Button Content="打开" Margin="10" Height="30" Width="80" Click="Button_Click_1"/>
        </StackPanel>
    </Grid>
</Window>
